{
  "name": "workout-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,scss,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,scss,css,md}\"",
    "type-check": "tsc --noEmit",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:critical": "vitest run --config vitest.config.critical.ts",
    "test:ui": "vitest --ui",
    "verify": "npm run lint && npm run type-check && npm run test:run && npm run build",
    "verify:quick": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "classnames": "^2.5.1",
    "lucide-react": "^0.263.1",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/aos": "^3.0.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.27",
    "prettier": "^3.2.4",
    "sass": "^1.89.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4"
  }
}
